{
  "name": "siege-ui",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "build": "pnpm typecheck && vite build",
    "dev": "vite",
    "lint": "eslint src",
    "postinstall": "husky install",
    "serve": "vite preview",
    "typecheck": "pnpm vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,vue,ts,html,json,json5,graphql}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "clipboard-copy": "4.0.1",
    "fathom-client": "3.2.0",
    "graphql": "16.6.0",
    "vue": "3.2.39"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@vitejs/plugin-vue": "3.1.0",
    "@vue/compiler-sfc": "3.2.39",
    "browserslist": "4.21.4",
    "esbuild-plugin-browserslist": "0.5.0",
    "eslint": "8.23.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "1.54.9",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "vue-tsc": "0.40.13"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/node",
        "esbuild",
        "ts-node"
      ]
    }
  }
}
