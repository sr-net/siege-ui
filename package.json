{
  "name": "siege-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "vite",
    "lint": "eslint src",
    "postinstall": "husky install",
    "serve": "vite preview",
    "typecheck": "yarn vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,vue,ts,html,json,json5,graphql}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "clipboard-copy": "4.0.1",
    "fathom-client": "3.0.0",
    "graphql": "15.5.0",
    "vue": "3.0.11"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.2.0",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@vitejs/plugin-vue": "1.2.3",
    "@vue/compiler-sfc": "3.0.11",
    "browserslist": "4.16.6",
    "esbuild-plugin-browserslist": "0.1.0",
    "eslint": "7.27.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "sass": "1.34.1",
    "typescript": "4.3.2",
    "vite": "2.3.6",
    "vite-tsconfig-paths": "3.3.13",
    "vue-tsc": "0.1.7"
  }
}
