{
  "name": "siege-ui",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": "^20"
  },
  "scripts": {
    "build": "pnpm typecheck && vite build",
    "dev": "vite",
    "lint": "eslint src",
    "postinstall": "husky install",
    "serve": "vite preview",
    "typecheck": "pnpm vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,vue,ts,html,json,json5,graphql}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "clipboard-copy": "4.0.1",
    "fathom-client": "3.5.0",
    "graphql": "16.8.1",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/compiler-sfc": "3.3.4",
    "browserslist": "4.22.1",
    "esbuild-plugin-browserslist": "0.9.1",
    "eslint": "8.51.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "sass": "1.69.1",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-plugin-webfont-dl": "3.7.6",
    "vite-tsconfig-paths": "4.2.1",
    "vue-tsc": "1.8.18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/node",
        "esbuild",
        "ts-node"
      ]
    }
  }
}
