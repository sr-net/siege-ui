{
  "name": "siege-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "vite",
    "lint": "eslint src",
    "postinstall": "husky install",
    "serve": "vite preview",
    "typecheck": "yarn vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,vue,ts,html,json,json5,graphql}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "clipboard-copy": "4.0.1",
    "fathom-client": "3.2.0",
    "graphql": "15.8.0",
    "vue": "3.2.19"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.3.0",
    "@graphql-codegen/add": "3.1.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.5",
    "@vitejs/plugin-vue": "1.9.3",
    "@vue/compiler-sfc": "3.2.19",
    "browserslist": "4.17.1",
    "esbuild-plugin-browserslist": "0.2.1",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.0",
    "prettier": "2.4.1",
    "sass": "1.42.1",
    "typescript": "4.4.3",
    "vite": "2.6.3",
    "vite-tsconfig-paths": "3.3.14",
    "vue-tsc": "0.3.0"
  }
}
